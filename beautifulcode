#include <stdio.h>

/**
 * multiply - multiplication function
 * @a: the value to use in the multiplication
 * @b: the value to multiply with
 * Return: the multiplied value
 */
int multiply(int a, int b)
{
	return (a * b);
}

/**
 * main - Entry point
 *
 * Description: Prints a beautiful message
 *              using printf
 * Return: Always 0 (Success)
 */
int main(void)
{
	int c;

	c = multiply(20, 30);
	printf("The result of the value is: %d\n", c);
	return (0);
}

